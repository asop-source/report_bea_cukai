<odoo>
  <data>
        <record id="view_master_data_tree" model="ir.ui.view">
            <field name="name">master.data.doc.tree</field>
            <field name="model">master.data.doc</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Master Dokumen">
                    <field name="nomor"/>
                    <field name="name"/>
                </tree>
            </field> 
        </record>


        <record id="view_master_data_form" model="ir.ui.view">
            <field name="name">master.data.doc.form</field>
            <field name="model">master.data.doc</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Master Dokumen">
                    <sheet>
                        <group>
                            <field name="nomor"/>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
  </data>
</odoo>